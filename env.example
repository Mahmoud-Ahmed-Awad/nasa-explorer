# NASA Explorer Environment Variables

# API Configuration
VITE_API_BASE=/api
VITE_NASA_API_KEY=your_nasa_api_key_here

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true
VITE_ENABLE_PWA=true

# Development Settings
VITE_DEV_MODE=true
VITE_MOCK_API=true

# NASA API Endpoints (when using real NASA APIs)
VITE_NASA_APOD_API=https://api.nasa.gov/planetary/apod
VITE_NASA_NEO_API=https://api.nasa.gov/neo/rest/v1
VITE_NASA_MARS_WEATHER_API=https://api.nasa.gov/insight_weather

# AI Assistant Configuration
VITE_AI_API_ENDPOINT=/api/ai/chat
VITE_AI_MODEL=gpt-3.5-turbo
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Performance Settings
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_CODE_SPLITTING=true
VITE_BUNDLE_ANALYZER=false

# Analytics (Optional)
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Social Media
VITE_TWITTER_HANDLE=@nasaexplorer
VITE_GITHUB_URL=https://github.com/your-username/nasa-explorer

# Contact Information
VITE_CONTACT_EMAIL=contact@nasa-explorer.com
VITE_SUPPORT_EMAIL=support@nasa-explorer.com
